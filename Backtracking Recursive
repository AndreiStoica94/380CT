#include <iostream>
using namespace std;
int x[100],i, t,n,k,nrsol,a[100];
void read()
{
cin>>n>>t;
for(i=1;i<=n;i++)
    cin>>a[i];
}
void printx()
{
 int i; cout<<endl;nrsol++;
 for(i=1;i<=n;i++) cout<<x[i]<<" ";
}
int sum(int k)
{
    int s=0;
    for(i=1;i<=k;i++)s=s+a[i]*x[i];
    return s;
}
int valid(int k)
{ 
return t>=sum(k);
}
void back(int k)
{
if(k==n+1){ if(sum(n)==t)printx();}
 else
      for(int i=0; i<=1;i++)
       {
        x[k]=i;
        if(valid(k)) back(k+1);
        }
}
int main()
{
read();  back(1);
cout<<endl<<"nr.sol="<<nrsol;
return 0;
}
