#include <iostream>
using namespace std;
int main()
{   
      int n,a[200],x[200],aux,i,j,s,t;
      cout<<"n=";cin>>n;
      for(i=1;i<=n;i++)
              cin>>a[i];
       cout<<"target=";cin>>t;
       for(i=1;i<n;i++)
             for(j=i+1;j<=n;j++)
                  if(a[i]<a[j]){
                                aux=a[i];  
                                a[i]=a[j];
                                a[j]=aux;
                               }
    j=1;    s=0;
    for(i=1;i<=n;i++)
        if(s+a[i]<=t)
        {    
        x[j++]=a[i];  s+=a[i];
        }   
    cout<<endl;
    for(i=1;i<j;i++)cout<<x[i]<<" ";
}
